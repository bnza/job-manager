{
  "name": "bnza/job-manager",
  "license": "GPL-3.0-or-later",
  "type": "symfony-bundle",
  "description": "Simple job manager",
  "keywords": [
    "job",
    "task",
    "runner"
  ],
  "require-dev": {
    "phpunit/phpunit": "~7"
  },
  "autoload": {
    "psr-4": {
      "Bnza\\JobManagerBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bnza\\JobManagerBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "symfony/console": "^4.2",
    "symfony/dependency-injection": "^4.2",
    "symfony/config": "^4.2",
    "symfony/yaml": "^4.2",
    "symfony/framework-bundle": "^4.2",
    "doctrine/inflector": "^1.3"
  }
}
