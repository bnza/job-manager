{
  "name": "bnza/job-manager",
  "license": "GPL-3.0-or-later",
  "type": "symfony-bundle",
  "description": "Simple job manager",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "keywords": [
    "job",
    "task",
    "runner"
  ],
  "require-dev": {
    "phpunit/phpunit": "~7",
    "friendsofphp/php-cs-fixer": "~2",
    "phpmd/phpmd": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Bnza\\JobManagerBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bnza\\JobManagerBundle\\Tests\\": "tests/unit/"
    }
  },
  "require": {
    "php": "~7.1",
    "ext-json": "*",
    "ext-posix": "*",
    "symfony/serializer": "~4.4",
    "jawira/case-converter": "~3.4"
  },
  "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
